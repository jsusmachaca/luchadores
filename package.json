{
  "name": "luchadores",
  "version": "1.0.0",
  "description": "API for the \"Luchadores platform\", a open source project designed to help people with neoplastic diseases (cancer)",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "node --watch src/index.js",
    "gen-key": "sed -i \"s/^SECRET_KEY=.*/SECRET_KEY=$(uuidgen | base64)/\" .env",
    "test": "jest --verbose --silent --detectOpenHandles"
  },
  "author": "jsusmachaca",
  "license": "GPL-3.0",
  "dependencies": {
    "@aws-sdk/client-s3": "3.540.0",
    "@aws-sdk/s3-request-presigner": "3.540.0",
    "bcrypt": "5.1.1",
    "cors": "2.8.5",
    "ejs": "3.1.10",
    "express": "4.18.2",
    "jsonwebtoken": "9.0.2",
    "multer": "1.4.5-lts.1",
    "mysql2": "3.7.0",
    "pg": "8.11.3",
    "sharp": "0.33.4",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "dotenv": "^16.3.1",
    "jest": "^29.7.0",
    "standard": "^17.1.0",
    "supertest": "^6.3.4"
  },
  "eslintConfig": {
    "extends": "standard"
  }
}
